{
  "expo": {
    "name": "BiteInsight",
    "slug": "gcahill",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "biteinsight",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#e2f1ee"
    },
    "ios": {
      "newArchEnabled": true,
      "supportsTablet": false,
      "bundleIdentifier": "com.biteinsightapp.gcahill",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "BiteInsight needs camera access to scan food labels and barcodes.",
        "NSPhotoLibraryUsageDescription": "BiteInsight needs access to your photo library.",
        "NSFaceIDUsageDescription": "BiteInsight uses Face ID to let you sign in quickly and securely."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#e2f1ee"
      },
      "package": "com.biteinsightapp.gcahill",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.USE_BIOMETRIC",
        "android.permission.RECORD_AUDIO",
        "android.permission.USE_FINGERPRINT"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-secure-store",
      [
        "expo-camera",
        {
          "cameraPermission": "BiteInsight needs camera access to scan food labels and barcodes."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "BiteInsight needs access to your photo library to set your profile picture."
        }
      ],
      "expo-sqlite",
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "BiteInsight uses Face ID to let you sign in quickly and securely."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "dcda6136-ab96-4037-8f10-40fc2a71d25a"
      }
    },
    "owner": "biteinsightapp"
  }
}
